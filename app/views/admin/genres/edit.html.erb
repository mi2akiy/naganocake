<h4>ジャンル編集</h4>
<%= form_with model: @genre, url:admin_genre_path, method: :patch do |f| %>
  <div class="row form-inline m-2">
      <div class="col-4">
        <%= f.label :ジャンル名 %>
      </div>
      <div>
        <%= f.text_field :name, autofocus: true, id:"name" %>
      </div>
      <div>
        <%= f.submit "変更を保存" %>
      </div>
  </div>
<% end %>